#include <stdio.h>

int main(){
	
	int n,k,d,sum=0;
	int a[120][120];
	
	scanf("%d %d %d",&n,&k,&d);
	
	for(int i=0;i<n+2;i++){
		for(int j=0;j<n+2;j++) a[i][j]=-1;
	}
	
	for(int i=1;i<=n;i++){
		for(int j=1;j<=n;j++) scanf("%d",&a[i][j]);
	}
	
	for(int y=0;y<k;y++){ //y=ì¼
		
		for(int i=1;i<=n;i++){
			
			for(int j=1;j<=n;j++){
				
				if(a[i][j]>d) a[i][j]=-1;
				
				if(a[i][j]==0){
					
					if(a[i+1][j]>=1 || a[i-1][j]>=1 || a[i][j+1]>=1 || a[i][j-1]>=1) a[i][j]=-2;
					
				}
				
				else if(a[i][j]!=-1) a[i][j]++;
			}
		}
		
		for(int i=1;i<=n;i++){
			for(int j=1;j<=n;j++){
				if(a[i][j]==-2) a[i][j]=1;
			}
		}
	}
	
	for(int i=1;i<=n;i++){
		for(int j=1;j<=n;j++){
			if(a[i][j]>=1) sum++;
		}
	}
	
	printf("%d",sum);
				
		
}
	
